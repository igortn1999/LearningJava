package sec5_ClassesMetodos.desafioModulo;

public class Jantar {

	public static void main(String[] args) {
		
		Pessoa braia = new Pessoa("Braia", 57);
		Comida borsh = new Comida("Borsh", 5);
		
		System.out.println("Braia tá magrelo, tem só "+ braia.peso + "Kg");
		System.out.println("Braia vai comer 5Kg de Borsh: ");
		
		braia.comer(borsh);
		
		System.out.println("Braia agora tem "+ braia.peso + "Kg");
		
	}

}

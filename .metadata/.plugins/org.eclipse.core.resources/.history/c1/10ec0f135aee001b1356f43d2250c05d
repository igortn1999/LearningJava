package sec8_Lambdas.desafio_BinaryOperator;

import java.util.function.Function;

import sec8_Lambdas.ProdutoLambda;

public class Desafio_BO {

	public static void main(String[] args) {
		
		ProdutoLambda p = new ProdutoLambda("Ipad", 3235.89, 0.13);
		
		Function<ProdutoLambda, Double> precoReal = 
				prod -> prod.getPreco() * prod.getDesconto();
				
		Function<ProdutoLambda, Double> 
			imposto = p -> p.getPreco()>=2500? precoReal.apply(p)*(1-0.085) : ;
		
		
		
	}

}

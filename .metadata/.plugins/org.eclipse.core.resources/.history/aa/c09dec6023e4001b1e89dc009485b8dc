package cap1_fundamentos;

public class AreaCircunferencia {

	public static void main(String[] args) {
		
		double raio = 3.4;
		
		final double PI = 3.14159; //variavel agora está constante, geralmente constante é com todas as letras em caps
		
		double area = PI * raio * raio;
		
		System.out.println("area: "+ area);
		
		
		
	}

}

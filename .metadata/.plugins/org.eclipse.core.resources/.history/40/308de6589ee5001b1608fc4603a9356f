package sec5_ClassesMetodos.desafioClasseData;

public class ClasseData {
	
	int dia;
	int mes;
	int ano;
	
	ClasseData(){
		
	}
	
	String obterData() {
		
		String data =String.format("%d/%d/%d", dia, mes, ano);
		
		//Metodo Nikita:
		//String data = dia + "/" + mes + '/'+ ano;
		
		return data;
	}
}

		Scanner sc = new Scanner(System.in);
		
		System.out.println("Qtd de notas: ");
		double[] notas = new double[sc.nextInt()];
		
		for(double nota : notas) {
			System.out.println("Digite uma nota: ");
			
		}
		
		sc.close();
		
	}

}

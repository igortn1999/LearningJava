package sec8_Lambdas;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.function.Consumer;

public class Consumidor {
	
	public static void main(String[] args) {
		
		Consumer<ProdutoLambda> imprimir = p -> System.out.println(p.nome); 
		
		ProdutoLambda p1 = new ProdutoLambda("Caneta", 12.34, 0.09);
		imprimir.accept(p1);
		
		ProdutoLambda p2 = new ProdutoLambda("Caneta", 12.34, 0.09);
		ProdutoLambda p3 = new ProdutoLambda("Caneta", 12.34, 0.09);
		ProdutoLambda p4 = new ProdutoLambda("Caneta", 12.34, 0.09);
		ProdutoLambda p5 = new ProdutoLambda("Caneta", 12.34, 0.09);

		
		List<ProdutoLambda> produtos = Arrays.asList(p1, p2, p3, p4, p5);
		
		
	}

}

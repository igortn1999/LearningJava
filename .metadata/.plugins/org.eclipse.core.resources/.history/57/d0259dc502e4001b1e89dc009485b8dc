package cap1.fundamentos;

public class TiposPrimitivos {

	public static void main(String[] args) {
		
		
		byte b; //composto por um byte (8 bits)
		short s; //composto por 2 bytes 
		int i; //composto por 4 bytes
		long l; //composto por 8 bytes
		
		float f; //4 bytes
		double d; //8 bytes
		
		char c; 
		
		boolean b; //(false ou true)
	}

}

package cap1.fundamentos;

public class PrimitivoVsObjeto {

	public static void main(String[] args) {
		
		//primitivos já foram tratados.
		
		String s = "Ola"; //o objeto "s" é criado a partir da classe "String".
						  //s agora tem o valor "Ola" atribuido
		
		s.toUpperCase();
		System.out.println(s);
		
		//tipos primitivos não tem notação ponto

	}

}

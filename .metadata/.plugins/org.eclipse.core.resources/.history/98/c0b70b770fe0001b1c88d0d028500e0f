package exsResolvidos;

import java.util.Scanner;

public class Ex9Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int dia = sc.nextInt();
		
		switch (dia) {
		
		case 1:
			System.out.println("Domingo");
			break;
		
		default:
			System.out.println("Valor inv√°lido!");
			break;
		}
		
	}

}

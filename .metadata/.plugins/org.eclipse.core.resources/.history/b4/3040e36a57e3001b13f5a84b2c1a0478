package aulaMatriz;

import java.util.Scanner;

public class MatrizMain {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		double[][] mDouble;
		
		int M = sc.nextInt();
		int N = sc.nextInt();
		
		
		
		mDouble = new double[M][N];//[linhas][colunas]

	}

}

package sec8_Lambdas;

import java.util.function.Predicate;

public class Predicado {
	
	public static void main(String[] args) {
		
	Predicate<ProdutoPredicado> isCaro = prod -> false;
	
	ProdutoPredicado prod = new ProdutoPredicado("Notebook", 3999.98, 0.15);
	System.out.println(isCaro.test());
		
	}

}

		mes = 1;
		ano = 1970;
	}
	
	ClasseData(int diaI, int mesI, int anoI){
		
	}
	
	String obterData() {
		
		String data =String.format("%d/%d/%d", dia, mes, ano);
		
		//Metodo Nikita:
		//String data = dia + "/" + mes + '/'+ ano;
		
		return data;
	}
}

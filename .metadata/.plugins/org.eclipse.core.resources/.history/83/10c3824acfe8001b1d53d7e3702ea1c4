package sec7_OrientacaoObjeto.composicao;

import java.util.ArrayList;
import java.util.List;


public class Aluno {
	
	final String nome;
	List<Curso> cursos = new ArrayList<>();
	
	Aluno(String nome){
		this.nome=nome;
	}
	
	void adicionarCursos(Curso curso){
		this.cursos.add(curso);
		curso.alunos.add(this);
	}

}

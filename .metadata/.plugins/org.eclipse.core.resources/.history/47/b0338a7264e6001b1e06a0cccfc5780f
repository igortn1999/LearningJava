package sec6_ArraysColecoes.arrays;

import java.util.Arrays;

public class Exercicio {

	public static void main(String[] args) {
		
		double[] notasAlunoA = new double[3];
		
		notasAlunoA[0]=7.9;
		notasAlunoA[1]=8;
		notasAlunoA[0]=6.7;
		
		System.out.println(Arrays.toString(notasAlunoA));
		
		double total=0.0;
		for(int i=0;i<3;i++) {
			System.out.println("");
		}
		
		
	}

}

package aulaMatriz;

import java.util.Scanner;

public class Ex2Matriz {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int M = sc.nextInt();
		int N = sc.nextInt();
		
		double[][] mat = new double[M][N];
		
		for(int i=0;i<n;i++) {
			for(int j=0;j<m;j++) {
				mat[i][j]=sc.nextDouble();
			}
		}
		
		

	}

}

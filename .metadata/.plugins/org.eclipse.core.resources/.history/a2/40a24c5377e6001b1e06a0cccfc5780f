package sec6_ArraysColecoes;

public class Usuario {
	
	String nome;
	String email;
	
	Usuario(){
		this("Default", "default@domain.com");
	}
	
	Usuario(String nome, String email){
		this.nome=nome;
		this.email=email;
		
	}
	
	@Override
	public boolean equals(Object objeto) {//Object Ã© a classe da qual todos os objetos herdam
	
		Usuario outro = (Usuario) objeto;
		
		booleanomeIgual = outro.nome == this.nome;
		
		return outro.nome == this.nome;
	
	}

}
